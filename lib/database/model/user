class User{


  int _level;
  int _daysInRow;

  User(this._level, this._daysInRow);

  User.map(dynamic obj){

    this._level = obj['level'];
    this._daysInRow = obj['daysInRow'];

  }

  int get level => _level;
  int get daysInRow => _daysInRow;

  Map<String, dynamic> toMap(){

    var map = new Map<String, dynamic>();

    map['level'] = _level;
    map['daysInRow'] = _daysInRow;

    return map;

  }

}